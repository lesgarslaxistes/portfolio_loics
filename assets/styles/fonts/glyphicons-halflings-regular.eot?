		<script>
		// Recupere l'url de la page en cours, mais supprime la variable d'url de langue si elle existe déjà
		var curr_page = window.location.href.split("?lg")[0].split("&lg")[0], 
			// Déclare les nouvelles variables
			next_pagefr = "",
			next_pageen = "";
			next_pagees = "";
			
		// Si la page a deja une variable d'url, ajoute la nouvelle variable avec &variable=
		// sinon, crée la premiere variable avec ?variable=
		if(curr_page.indexOf("?") > -1) {
			next_pagefr = curr_page+"&lg=fr";
			next_pageen = curr_page+"&lg=en";
			next_pagees = curr_page+"&lg=es";
		} else {
			next_pagefr = curr_page+"?lg=fr";
			next_pageen = curr_page+"?lg=en";
			next_pagees = curr_page+"?lg=es";
		}
		</script>
